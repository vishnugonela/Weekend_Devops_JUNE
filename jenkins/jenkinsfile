pipeline {
    agent any
    stages{
        stage('listing_scripts'){
            steps {
                sh''' 
                     ' echo listing scripts from github repo'
               ls ${WORKSPACE}/'''
            }
        }
         stage('executing_scripts'){
            steps {
                sh '''
                      'echo Build is sucessful on stage2'
               ls ${WORKSPACE}/linux/script.sh '''
            }
        }
    }
}
